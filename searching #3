class Solution{
        
    static int search(int arr[], int N, int X)
    {
       //traverse array and check if x present if so return i
       //i is position started from 0
        for(int i=0;i<N;i++)
        {
            if(arr[i]==X)
             return i;
        }
        //if element not found return -1
     return -1;   
    }
    
}
